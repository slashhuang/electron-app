{
  "private": true,
  "scripts": {
    "dev":"webpack",
    "dev": "concurrently --kill-others \"npm run hot\" \"npm start\"",
    "hot": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "electron ./app/main.js",
    "pack": "build",
    "clean": "rimraf dist",
    "clean:osx": "rimraf dist/Onshape-darwin-x64",
    "clean:win": "rimraf dist/win"
  },
  "build": {
    "app-bundle-id": "slashhuang",
    "app-category-type": "public.app-category.graphics-design",
    "osx": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "electron-builder": "^3.14.0",
    "electron-hot-loader": "^0.8.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.8",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.8",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1"
  }
}
